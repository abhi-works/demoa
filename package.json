{
  "name": "demoa",
  "version": "0.1.0",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "react-scripts start",
    "comj": "ncp src/ dist/cjs",
    "comja": "copyfiles -u 1 \"src/**/*.js\" dist/",
    "comjab": "copyfiles -u 2 \"src/**/*.js\" fg",
    "build": "rm -rf /dist && npm run build:cjs && npm run comj && npm run comja  && npm run comjab",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonJS --outDir dist/cjs",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "copyfiles": "^2.4.1",
    "ncp": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rimraf": "^4.1.2",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
